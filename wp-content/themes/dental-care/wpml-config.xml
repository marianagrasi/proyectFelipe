<wpml-config>
    <custom-fields>
        <custom-field action="ignore">gallery_images_img</custom-field>
        <custom-field action="ignore">gallery_select</custom-field>
        <custom-field action="ignore">header_image_en</custom-field>
        <custom-field action="ignore">page_breadcrumben</custom-field>
        <custom-field action="ignore">page_header_bg_img</custom-field>
        <custom-field action="ignore">page_header_bg_overlay</custom-field>
        <custom-field action="ignore">page_header_en</custom-field>
        <custom-field action="ignore">page_header_select</custom-field>
        <custom-field action="ignore">page_layout</custom-field>
        <custom-field action="ignore">page_title_align</custom-field>
        <custom-field action="ignore">post_gallery_images_img</custom-field>
        <custom-field action="ignore">primary_sidebar</custom-field>
        <custom-field action="ignore">service_gallery_type</custom-field>
        <custom-field action="translate">service_desc</custom-field>
        <custom-field action="translate">team_member_bio</custom-field>
        <custom-field action="translate">team_member_details_list</custom-field>
        <custom-field action="translate">team_member_pos</custom-field>
        <custom-field action="ignore">team_member_social_list</custom-field>
        <custom-field action="translate">testimonialname</custom-field>
        <custom-field action="translate">testimonialposition</custom-field>
        <custom-field action="translate">testimonialtext</custom-field>
    </custom-fields>
    <custom-types>
        <custom-type translate="1">brand</custom-type>
        <custom-type translate="1">gallery</custom-type>
        <custom-type translate="1">service</custom-type>
        <custom-type translate="1">team-member</custom-type>
         <custom-type translate="1">testimonial</custom-type>
    </custom-types>    
    <taxonomies>
        <taxonomy translate="1">gallery-categories</taxonomy>
    </taxonomies>       
    <language-switcher-settings>
        <key name="icl_lang_sel_config">
            <key name="font-current-normal">#444444</key>
            <key name="font-current-hover">#000000</key>
            <key name="background-current-normal">#ffffff</key>
            <key name="background-current-hover">#eeeeee</key>
            <key name="font-other-normal">#444444</key>
            <key name="font-other-hover">#000000</key>
            <key name="background-other-normal">#ffffff</key>
            <key name="background-other-hover">#eeeeee</key>
            <key name="border">#cdcdcd</key>
        </key>
        <key name="icl_lang_sel_footer_config">
            <key name="font-current-normal">#444444</key>
            <key name="font-current-hover">#000000</key>
            <key name="background-current-normal">#ffffff</key>
            <key name="background-current-hover">#eeeeee</key>
            <key name="font-other-normal">#444444</key>
            <key name="font-other-hover">#000000</key>
            <key name="background-other-normal">#ffffff</key>
            <key name="background-other-hover">#eeeeee</key>
            <key name="border">#cdcdcd</key>
        </key>    
        <key name="icl_language_switcher_sidebar">0</key>  
        <key name="icl_widget_title_show">0</key>
        <key name="icl_lang_sel_type">dropdown</key>
        <key name="icl_lso_link_empty">0</key>
        <key name="icl_lso_flags">0</key>
        <key name="icl_lso_native_lang">1</key>
        <key name="icl_lso_display_lang">1</key>
        <key name="icl_lang_sel_footer">0</key>
        <key name="icl_post_availability">0</key>
        <key name="icl_post_availability_position">below</key>
        <key name="icl_post_availability_text">This post is also available in: %s</key>    
    </language-switcher-settings>    
</wpml-config>


